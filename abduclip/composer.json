{
    "name": "abdurahman/mon-projet-jeux",
    "description": "Projet de plateforme de mini-jeux en ligne.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdurahman",
            "email": "abdu.usdi@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "mongodb/mongodb": "^1.8",
        "twig/twig": "^3.0",
        "symfony/http-foundation": "^6.0",
        "symfony/routing": "^6.0",
        "symfony/security-core": "^6.0",
        "symfony/security-http": "^6.0",
        "phpmailer/phpmailer": "^6.5",
        "google/apiclient": "^2.18"
    },
    "autoload": {
        "psr-4": {
            "Database\\": "src/Database/",
            "Controllers\\": "src/Controllers/",
            "Models\\": "src/Models/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@composer dump-autoload"
        ],
        "post-update-cmd": [
            "@composer dump-autoload"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
